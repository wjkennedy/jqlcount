<?xml version="1.0" encoding="UTF-8"?>

<atlassian-plugin key="a9.jqlcount-tests" name="jqlcount" plugins-version="2"> 
  <plugin-info> 
    <description>This is the a9:jqlcount plugin for Atlassian JIRA.</description>  
    <version>1.0.0-SNAPSHOT</version>  
    <vendor name="Example Company" url="http://www.example.com/"/> 
  </plugin-info>  
  <!-- from our base plugin -->  
  <component-import key="myComponent" interface="a9.api.MyPluginComponent"/>  
  <!-- from the product container -->  
  <component-import key="applicationProperties" interface="com.atlassian.sal.api.ApplicationProperties"/>  
  <component key="my-component-wired-test" class="it.a9.MyComponentWiredTest" public="true">
    <interface>it.a9.MyComponentWiredTest</interface>
    <service-properties>
      <entry key="inProductTest" value="true"/>
    </service-properties>
  </component>
</atlassian-plugin>
